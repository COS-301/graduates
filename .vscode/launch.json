  "version": "0.2.0",
  "configurations": [
    {
      "name": "api",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "yarn",
      "runtimeArgs": ["start", "api"],
      "outFiles": ["${workspaceFolder}/dist/apps/api/**/*.js"]
    },
    {
      "type": "pwa-chrome",
      "request": "launch",
      "name": "client",
      "url": "http://localhost:4200",
      "webRoot": "${workspaceFolder}/dist/apps/client"
    }
  ]
}
